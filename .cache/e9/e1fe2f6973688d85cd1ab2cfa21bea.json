  background: rgba(0, 0, 0, .7);\n    padding: 2rem;\n    max-width: 1000px;\n    \n    &_title {\n        grid-column: 1/-1;\n    }\n\n    &_date {\n        grid-column: 1/2;\n\n        &__day {\n            font-size: $fs-h2;\n            color: $clr-gray;\n            font-weight: $fw-light;\n            margin: 0;\n        }\n    }\n\n    &_temp {\n        grid-column: 2/3;\n        padding: 0 2em;\n        @include border-right-left;\n    }\n\n    &_wind {\n        grid-column: 3/4;\n        display: grid;\n        grid-template-columns: repeat(2, 1fr);\n        grid-template-rows: min-content, 1fr;\n        align-self: start;\n\n        .wind {\n            &__direction {\n                $size : 6rem;\n                width: $size;\n                height: $size;\n                border-radius: 50%;\n                background-color: rgba(255, 255, 255, 0.5);\n                display: grid;\n                place-items: center;\n                grid-row: 1/ span 2;\n            }\n\n            &__arrow {\n                --direction:0deg;\n                $size: 1rem;\n                width: $size;\n                height: $size*3;\n                background-color: $clr-accent-dark;\n                clip-path: polygon(50% 0%, 0% 100%, 100% 100%);\n                transform-origin: bottom center;\n                transform: translateY(-50%)  rotate(var(--direction));\n            }\n        }\n    }\n\n    &_info {\n        grid-column: 1/3;\n    }\n\n    &_unit {\n        grid-column: 3/4;\n        place-self: end;\n        color: $clr-light;\n        display: flex;\n        align-items: center;\n        opacity: .6;\n        transition: opacity 300ms linear;\n\n        &:hover {\n            opacity: 1;\n        }\n        \n        label {\n            cursor: pointer;\n        }\n\n        input {\n            clip: rect(0 0 0 0);\n            clip-path: inset(50%);\n            height: 13px;\n            overflow: hidden;\n            position: absolute;\n            white-space: nowrap;\n            width: 13px;\n        }\n\n        .unit__toggle {\n            cursor: pointer;\n            border: 2px solid $clr-light;\n            background-color: transparent;\n            width: 4em;\n            border-radius: 100vmax;\n            padding: 0;\n            margin: 0 .5rem;\n            outline: none;\n\n            &::after {\n                content: '';\n                display: block;\n                height: 1rem;\n                margin: 2px;\n                margin-left: auto;\n                width: 1rem;\n                background-color: $clr-light;\n                border-radius: 50%;\n            }\n        }\n\n        :checked ~ .unit__toggle::after {\n            margin-left: 2px;\n        }\n    }\n}\n\n.reading {\n    font-size: $fs-h1;\n    color: $clr-gray;\n    margin: 0;\n}\n\n.previous-wheather {\n    background: $clr-light;\n    color: $clr-dark;\n    position: absolute;\n    width: 100%;\n    bottom: 0;\n    transform: translateY(75%);\n    transition: transform 500ms ease-in-out;\n    padding: 1rem 3rem;\n\n    &_show{\n        position: absolute;\n        background-color: $clr-light;\n        left: 50%;\n        width: 10rem;\n        transform: translateX(-50%) translateY(calc(-99% - 1rem)) rotate(0deg);\n        clip-path: polygon(50% 0%, 0% 100%, 100% 100%);\n        outline: none;\n        border: none;\n        cursor: pointer;\n        line-height: 1;\n        font-size: $fs-h2;\n        font-family: inherit;\n        color: $clr-gray;\n\n        &:hover,\n        &:focus {\n            color: $clr-dark;\n        }\n\n        span {\n            display: block;\n            transition: transform 350ms ease-in-out;\n        }\n\n        .rotate {\n            transform: translateY(6px) rotate(180deg);\n        }\n    }\n\n    &__title {\n        text-align: center;\n    }\n\n    .previous-day {\n        opacity: 0;\n\n        & > * {\n            margin: 0.5em;\n        }\n\n        &__date {\n            font-size: 0.9rem;\n            color: $clr-gray;\n        }\n\n        &__more-info {\n            border: 0;\n            cursor: pointer;\n            border-radius: 100vmax;\n            background-color: $clr-dark;\n            color: $clr-light;\n            text-transform: uppercase;\n            padding: .5rem 1rem;\n\n            &:hover {\n                background-color: $clr-gray;\n            }\n        }\n\n        \n    }\n}\n\n.show {\n    transform: translateY(1%);\n\n    .previous-wheather__title {\n        text-align: left;\n        animation: slideUp 700ms forwards;\n    }\n\n    .previous-day {\n        animation: slideUp 700ms forwards;\n    }\n\n    @for $i from 1 through 7 {\n        .previous-day:nth-child(#{$i}) { animation-delay: $i*80ms;}\n    }\n    \n}\n\n\n.previous-days {\n    display: flex;\n    justify-content: space-between;\n    padding: 2rem 3rem;\n}\n\n@media (min-width: 900px ) {\n\n    $fs-h1:1.25rem;\n    $fs-xl: 4rem;\n    $fs-body:1rem;\n\n    body {\n        height: 100vh;\n        overflow: hidden;\n    }\n\n    .mars-current {\n        margin: 4rem 0 0 4rem;\n        display: grid;\n        grid-template-columns: repeat(3, 1fr);\n        grid-gap: 2rem;\n    }\n}"},"lineCount":null}},"error":null,"hash":"fcb17ec3162b0249533cb90bd9ca0567","cacheData":{"env":{}}}